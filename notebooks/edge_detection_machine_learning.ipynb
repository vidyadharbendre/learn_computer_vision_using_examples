   "source": [
    "import cv2\n",
    "import numpy as np\n",
    "from sklearn.ensemble import RandomForestClassifier\n",
    "from skimage import color, filters, feature\n",
    "from skimage.util import view_as_windows\n",
    "\n",
    "# Step 1: Extract Features\n",
    "def extract_features(patch):\n",
    "    # Convert patch to grayscale\n",
    "    gray_patch = color.rgb2gray(patch)\n",
    "    \n",
    "    # Texture (e.g., edges with Sobel)\n",
    "    sobel_x = filters.sobel_h(gray_patch)\n",
    "    sobel_y = filters.sobel_v(gray_patch)\n",
    "    texture = np.sqrt(sobel_x**2 + sobel_y**2).flatten()\n",
    "    \n",
    "    # Brightness (mean intensity)\n",
    "    brightness = np.array([gray_patch.mean()])\n",
    "    \n",
    "    # Color (flattened RGB values)\n",
    "    color_features = patch.flatten()\n",
    "    \n",
    "    # Combine all features into a single array\n",
    "    features = np.hstack([texture, brightness, color_features])\n",
    "    return features\n",
    "\n",
    "# Step 2: Prepare Training Data\n",
    "def prepare_training_data(images, edge_labels, patch_size=(16, 16)):\n",
    "    X, y = [], []\n",
    "    for img, label in zip(images, edge_labels):\n",
    "        # Extract patches and corresponding labels\n",
    "        img_patches = view_as_windows(img, patch_size, step=patch_size[0])\n",
    "        label_patches = view_as_windows(label, patch_size, step=patch_size[0])\n",
    "        \n",
    "        for i in range(img_patches.shape[0]):\n",
    "            for j in range(img_patches.shape[1]):\n",
    "                patch = img_patches[i, j]\n",
    "                patch_label = label_patches[i, j]\n",
    "                \n",
    "                # Skip patches without edges if needed (optional)\n",
    "                if patch_label.mean() > 0:  # assuming 1 for edge, 0 for non-edge\n",
    "                    feature_vector = extract_features(patch)\n",
    "                    X.append(feature_vector)\n",
    "                    y.append(1 if patch_label.mean() > 0.5 else 0)  # binary edge label\n",
    "    return np.array(X), np.array(y)\n",
    "\n",
    "# Step 3: Train Random Forest Model\n",
    "def train_edge_detector(X, y):\n",
    "    rf_model = RandomForestClassifier(n_estimators=100, max_depth=10, random_state=42)\n",
    "    rf_model.fit(X, y)\n",
    "    return rf_model\n",
    "\n",
    "# Step 4: Predict Edges in New Image\n",
    "def detect_edges(image, model, patch_size=(16, 16)):\n",
    "    edge_map = np.zeros(image.shape[:2])\n",
    "    img_patches = view_as_windows(image, patch_size, step=patch_size[0])\n",
    "    \n",
    "    for i in range(img_patches.shape[0]):\n",
    "        for j in range(img_patches.shape[1]):\n",
    "            patch = img_patches[i, j]\n",
    "            feature_vector = extract_features(patch)\n",
    "            edge_label = model.predict([feature_vector])[0]\n",
    "            edge_map[i*patch_size[0]:(i+1)*patch_size[0], j*patch_size[1]:(j+1)*patch_size[1]] = edge_label * 255\n",
    "    \n",
    "    return edge_map\n",
    "\n",
    "# Step 5: Load Images and Labels (Example Data)\n",
    "# Assuming you have edge-labeled images and edge ground truth\n",
    "# images = [cv2.imread('path_to_image') for ...] \n",
    "# edge_labels = [cv2.imread('path_to_edge_label', 0) for ...]  # Load ground truth edge maps\n",
    "\n",
    "# Example with dummy data\n",
    "images = [np.random.rand(128, 128, 3)]  # Replace with actual images\n",
    "edge_labels = [np.random.randint(0, 2, (128, 128))]  # Replace with actual edge labels\n",
    "\n",
    "# Prepare training data\n",
    "X_train, y_train = prepare_training_data(images, edge_labels)\n",
    "\n",
    "# Train the model\n",
    "edge_detector = train_edge_detector(X_train, y_train)\n",
    "\n",
    "# Detect edges in a new image\n",
    "new_image = np.random.rand(128, 128, 3)  # Replace with an actual new image\n",
    "edges = detect_edges(new_image, edge_detector)\n",
    "\n",
    "# Step 6: Visualize the Output\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "plt.figure(figsize=(8, 4))\n",
    "plt.subplot(1, 2, 1)\n",
    "plt.imshow(new_image)\n",
    "plt.title(\"Original Image\")\n",
    "plt.axis(\"off\")\n",
    "\n",
    "plt.subplot(1, 2, 2)\n",
    "plt.imshow(edges, cmap=\"gray\")\n",
    "plt.title(\"Detected Edges\")\n",
    "plt.axis(\"off\")\n",
    "\n",
    "plt.show()\n"
   ]
